<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="mrs.database.UpcomingMovies" table="upcoming_movies">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id type="int" name="mid" column="mid">
      </id>
      <property name="title" column="title" type="string"/>
      <property name="year" column="year" type="int"/>
      <property name="year_end" column="year_end" type="int"/>
      <property name="vtype" column="vtype" type="string"/>
      <property name="notes" column="notes" type="string"/>
      <property name="rating" column="rating" type="float"/>
      <property name="num_votes" column="num_votes" type="int"/>
      <property name="distribution" column="distribution" type="string"/>
      <property name="year_suffix" column="year_suffix" type="string"/>
   </class>
</hibernate-mapping>